{
    "name": "kyrad/stack-csp",
    "description": "Stack middleware for generating Content Security Policy (CSP) 1.0 HTTP header(s) for the request ",
    "license": "MIT",
    "keywords": ["stack php", "symfony", "silex", "csp", "security", "content security policy"],
    "authors": [
        {
            "name": "Kyra D.",
            "email": "kyra@existing.me"
        }
    ],
    "require": {
        "php":         ">=5.4",
        "ornicar/php-user-agent": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit":           "~3.7",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls":    "~0.6"
    },
    "suggest": {
        "silex/silex": "Required for 'Csp' stack silex middleware usage"
    },
    "autoload": {
        "psr-0": {
            "KyraD\\Stack\\": "src"
        }
    }
}
